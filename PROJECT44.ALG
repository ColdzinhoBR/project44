Algoritmo "project44"



var
 numero : inteiro

 inicio
 escreval("digite um numero")
 leia(numero)
 

 se (numero > 0) entao
 escreva("numero positivo")

 se (numero < 0) entao
 escreva("numero negativo")
 
 fimse
 
 senao escreva ("numero e 0")

 
 FIMSE
 
 




fimalgoritmo